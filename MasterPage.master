    <title>Gasco - Tienda de artefactos</title>
    <script type="text/javascript" src="js/JQuery.js"></script>
    <link href="main.css" rel="stylesheet" />
    <script type="text/javascript">
        function setSearch(mv) {
            if (mv == 'Buscar productos') {
                $('#ctl00_txtSearch').val('');
            }
        }
        function leaveSearch(mv) {
            if (mv == '') {
                $('#ctl00_txtSearch').val('Buscar productos');
            }
        }
        function setDespacho() {
            $('#calculadesp').css('cursor', 'pointer');
            try {
                $("#ctl00_dcalcom option[value='Selecciona tu comuna']").remove();
            }
            catch (err) {
            }
            $("#calculadesp").click(function () {
                checkPrice();
            });
        }
        function checkPrice() {
            var mcom = $('#ctl00_dcalcom').val();
            var mcod = $('#ctl00_hfDesp').val();
            $('#resultDesp').html('cargando...');
            $.ajax({
                type: "POST",
                url: "checkPrecio.ashx",
                data: { com: mcom, cod: mcod }
            })
            .done(function (msg) {
                $('#resultDesp').html(msg);
            });
        }
    </script>


    <script type="text/javascript">
        function closetrama() {
            $('.layerterremoto').fadeOut("slow", function () {
                $('.tramahome').fadeOut("fast");
            });
        }
        function mostrarLayerRegistro() {
            $('.tramahome').fadeIn("fast", function () {
                $('.layerterremoto').fadeIn("fast");
            });
            localStorage.setItem('layerCBVBvisto', 4);
        }

       

        function abrirlightbox(){

            $('#fade').fadeIn();
            $('#light').fadeIn();
            $('.background-light-box').fadeIn();
        }

         function cerrarlightbox(){

            $('#fade').fadeOut();
            $('#light').fadeOut();
            $('.background-light-box').fadeOut();
        }

    </script>


    <style>
    .table {
        width: 800px;
        font-family:arial;
        font-size:12px;
        border-collapse: collapse;
    }
    .table, th, td {
       border: 1px solid #ddd;
       padding:2px;
    }
    .table th{
        background-color:#f1f1f1;
    }

    .tramahome { background-image:url(img/trama.gif); width:100%; height:100%;position:fixed;top:0;left:0;z-index:100; display:none; }
    .layerterremoto { background-image:url(img/norte20140404.png); width:645px; height:315px;position:fixed;top:50%;left:50%;z-index:110;margin-left:-322px; margin-top:-160px;display:none; }
    .closetrama { width:15px; height:15px;top:10px;right:10px;position:absolute;cursor:pointer; }

    .black_overlay{
        display: none;
        position: fixed;
        top: 0%;
        left: 0%;
        width: 100%;
        height: 100%;
        background-color: black;
        z-index:1001;
        -moz-opacity: 0.8;
        opacity:.80;
        filter: alpha(opacity=80);
    }
    .white_content {
        display: none;
        position: fixed;
        top: 50%;
        left: 50%;
        width: 461px;
        height: 401px;
       
        border: 0px solid;
        background: url(1.jpg);
        z-index:1002;
        overflow: auto;
        margin-left: -275px;
        margin-top: -200px;
    }


    .light{
        display: none;
        position: absolute;
        top: 25%;
        left: 25%;
        width: 920px;
        height: 543px;
        padding: 16px;
        border: 0px solid;
        background: url(2.jpg);
        overflow: hidden;
        z-index:1000000000;
        border-radius: 5px;
        box-shadow: 10px 10px 5px #888888;
    }

    .background-light-box{

        position: absolute;
        width: 100%;
        height: 100%;
        background-color: #666;
        z-index: 1000;
        opacity: 0.6;
        filter: alpha(opacity=60); /* For IE8 and earlier */
        display: none;

    }

    </style>

    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
</head>
<body>

    <div class="background-light-box"></div>
